{
    "name": "Word of the Tab",
    "description": "A chrome new tab extension that displays a random Spanish word and its translation",
    "version": "1.0.0",
    "chrome_url_overrides" : {
        "newtab": "spanTab.html"
    },

    "options_ui": {
        "page": "optionsPage.html",
        "chrome_style": true
    },

    "icons": {
        "128": "images/ext-icon.png"
    },

    "permissions": [
        "storage"
    ],

    "manifest_version": 2
 }