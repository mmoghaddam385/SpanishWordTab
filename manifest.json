{
    "name": "Spanish Word Tab",
    "description": "A chrome new tab extension that displays a random Spanish word and its translation",
    "version": "0.0.0.1",
    "chrome_url_overrides" : {
        "newtab": "spanTab.html"
    },

    "options_ui": {
        "page": "optionsPage.html",
        "chrome_style": true
    },

    "permissions": [
        "storage"
    ],

    "manifest_version": 2
 }